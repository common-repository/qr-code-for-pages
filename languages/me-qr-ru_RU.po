# {{ value }} - недопустимое значение проверки
# {{ prop }} - имя свойства с недопустимым значением

# <- Default validation message
msgid "Validation system error. Validation failed"
msgstr "Системная ошибка валидации. Валидация не удалась"

# %s - Невалидное сообщение
msgid "Validation system error. Validation failed. Error message: `%s`"
msgstr "Системная ошибка валидации. Валидация не удалась. Сообщение об ошибке: `%s`"

msgid "Validation errors"
msgstr "Ошибки валидации"

msgid "Data validation error"
msgstr "Ошибка проверки данных"

msgid "The specified value {{ value }} of the property {{ prop }}, is not valid"
msgstr "Указанное значение {{ value }} свойства {{ prop }} недопустимо"

msgid "The value `{{ value }}` should be of type {{ type }}, {{ invalid }} given"
msgstr "Значение `{{ value }}` должно быть типа {{ type }}, {{ invalid }} дано"

msgid "The value {{ value }} can contain a minimum of {{ min }} and a maximum of {{ max }} characters"
msgstr "Значение {{ value }} может содержать минимум {{ min }}  и максимум {{ max }} символов"

msgid "The specified property value is {{ prop }}, cannot be empty"
msgstr "Указанное значение свойства: {{ prop }}, не может быть пустым"

msgid "The specified property value is {{ prop }}, cannot be NULL"
msgstr "Указанное значение свойства {{ prop }}, не может быть NULL"

msgid "The value of {{ value }} does not match the format `{{ format }}`"
msgstr "Значение {{ value }} не соответствует формату `{{ format }}`"

msgid "The QR code format does not match the available formats"
msgstr "Формат QR кода не соответствует доступным форматам"
# <- END Default validation message


# <- Other validation message
msgid "QR block class must be string type, {{ invalid }} given"
msgstr "Класс QR-блока должен быть строковым типом, {{ invalid }} дано"

msgid "The QR block class must have a minimum of {{ min }} and a maximum of {{ max }} characters"
msgstr "Класс QR-блока должен иметь минимум {{ min }} и максимум {{ max }} символов"

msgid "QR image class must be string type, {{ invalid }} given"
msgstr "Класс QR изображения должен быть строковым типом, {{ invalid }} дано"

msgid "The QR image class must have a minimum of {{ min }} and a maximum of {{ max }} characters"
msgstr "Класс QR-изображения должен иметь минимум {{ min }} и максимум {{ max }} символов"

msgid "File logging value cannot be empty"
msgstr "Значение логирования файла не может быть пустым"

msgid "File logging value must be boolean type, {{ invalid }} given"
msgstr "Значение логирования файла должно быть boolean типа, {{ invalid }} дано"

msgid "Telegram logging value cannot be empty"
msgstr "Значение логирования Telegram не может быть пустым"

msgid "Telegram logging value must be boolean type, {{ invalid }} given"
msgstr "Значение логирования Telegram должно быть boolean типа, {{ invalid }} дано"
